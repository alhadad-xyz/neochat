{
  "name": "canistchat_frontend",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm i && dfx deploy",
    "start": "vite --port 3000",
    "dev": "vite --port 3000 --host",
    "prebuild": "dfx generate",
    "build": "tsc && vite build",
    "postbuild": "mkdir -p dist/embed && cp dist/index.html dist/embed/index.html",
    "build:prod": "VITE_DFX_NETWORK=ic npm run build",
    "preview": "vite preview --port 3000",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\"",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "analyze": "npx vite-bundle-analyzer",
    "clean": "rm -rf dist build node_modules/.vite",
    "ci": "npm run lint && npm run type-check && npm run test:unit && npm run build"
  },
  "dependencies": {
